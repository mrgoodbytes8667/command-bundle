{
    "name": "mrgoodbytes8667/command-bundle",
    "description": "A slightly better version of Symfony's base command class",
    "type": "symfony-bundle",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^7.4 || ^8.0",
        "symfony/console": "^5.2",
        "symfony/framework-bundle": "^5.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.8",
        "symfony/phpunit-bridge": "^5.2"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "suggest": {
        "doctrine/orm": "To use the BaseEntityManagerCommand class"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bytes\\CommandBundle\\": "src/",
            "Bytes\\CommandBundle\\Tests\\": "Tests/"
        }
    }
}
