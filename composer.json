{
    "name": "mrgoodbytes8667/command-bundle",
    "description": "A slightly better version of Symfony's base command class",
    "type": "symfony-bundle",
    "license": "CC-BY-NC-4.0",
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.2 | ^6.0",
        "symfony/framework-bundle": "^5.2 | ^6.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.8",
        "mrgoodbytes8667/pluralize-bundle": ">=2.0.0",
        "symfony/test-pack": "^1.0.9"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@goodbytes.live"
        }
    ],
    "suggest": {
        "doctrine/orm": "To use the BaseEntityManagerCommand class",
        "mrgoodbytes8667/pluralize-bundle": "For pluralizing counting tables"
    },
    "conflict": {
        "doctrine/annotations": "<1.11.1",
        "doctrine/cache": "<1.10.2",
        "doctrine/collections": "<1.6.7",
        "doctrine/event-manager": "<1.1.1",
        "doctrine/inflector": "<1.4.3",
        "doctrine/lexer": "<1.2.1",
        "doctrine/persistence": "<2.1.0",
        "phpunit/phpunit": "<9.5 || 9.5.8",
        "symfony/framework-bundle": "<5.2.1",
        "symfony/phpunit-bridge": "<5.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bytes\\CommandBundle\\": "src/",
            "Bytes\\CommandBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
